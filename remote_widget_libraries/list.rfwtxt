import core.widgets;
import core.material;

widget PatientList = ListView(
  children: [
    Text(text: "Your Patient list is"),
    ...for patient in data.server.patients:
      Patient(patient: patient)
  ],
);

widget Patient = ListTile(
  title: Text(text: args.patient.name),
  onTap: event 'patientSelect' { name: args.patient.name},
);
